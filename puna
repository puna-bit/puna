                break
            end
        end
        
        if not scriptLoaded then
            print("âŒ WORM-AI: KhÃ´ng thá»ƒ táº£i script. Kiá»ƒm tra láº¡i link!")
        end
    end)
    
    if not success then
        print("âŒ WORM-AI Lá»—i: " .. tostring(error))
        print("ğŸ”„ Thá»­ láº¡i sau 10 giÃ¢y...")
