        for i, link in ipairs(links) do
            print("ğŸ”„ WORM-AI: Äang thá»­ link " .. i .. "/" .. #links)
            
            local scriptContent = game:HttpGet(link)
            
            -- Kiá»ƒm tra script cÃ³ há»£p lá»‡ khÃ´ng
            if #scriptContent > 100 then
                loadstring(scriptContent)()
